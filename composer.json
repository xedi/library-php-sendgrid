{
    "name": "xedi/sendgrid",
    "description": "SendGrid PHP Library",
    "type": "library",
    "require": {
        "guzzlehttp/guzzle": "^6.3",
        "illuminate/support": "5.8.32"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.3",
        "squizlabs/php_codesniffer": "^3.4",
        "mockery/mockery": "^1.2",
        "fzaninotto/faker": "^1.8"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Chris Smith",
            "email": "chris@xedi.com"
        }
    ],
    "minimum-stability": "stable",
    "autoload": {
        "psr-4": {
            "Xedi\\SendGrid\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        },
        "classmap": [
            "src/",
            "tests/"
        ]
    },
    "suggest": {
        "Midders": "Wipes his shitty arse"
    }
